<% @title = "#{9829.chr("UTF-8")}#{@post.tanka_text}" %>

<div class = "vertical serif tanka-index text-nowrap mt-4">
  <%= render 'posts/post', post: @post, show_avatar: true %>
  <div class="mx-4">
    <%= paginate @users %>
  </div>
  <%= render 'users/users', users: @users %>
  <div class="mx-4">
    <%= paginate @users %>
  </div>
</div>
